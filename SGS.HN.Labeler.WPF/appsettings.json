{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=TWDB021;Initial Catalog=LIMS20_UAT;User ID=LIMS20TPEAdmin;Password=02SuperUser!@#$%^**()01.;Encrypt=False;Application Name=SGS.HN.Labeler"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [ // 輸出到
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "log/log-.txt", // 寫入檔案到 log 資料夾
          "rollingInterval": "Day" // 按天滾動檔案
        }
      },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://twtpeoad002:5341/" }
      }
    ],
    // 額外屬性，必須安裝對應 Enrichers 才能抓到，抓到才會輸出
    "Enrich": [ "WithEnvironmentName", "WithEnvironmentUserName", "WithMachineName", "WithProcessId", "WithThreadId", "WithAppVersion" ],
    "Destructure": [
      {
        "Name": "ToMaximumDepth",
        "Args": { "maximumDestructuringDepth": 4 }
      },
      {
        "Name": "ToMaximumStringLength",
        "Args": { "maximumStringLength": 100 }
      },
      {
        "Name": "ToMaximumCollectionCount",
        "Args": { "maximumCollectionCount": 10 }
      }
    ]
  }
}